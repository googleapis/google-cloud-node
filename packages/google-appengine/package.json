{
  "name": "@google-cloud/appengine-admin",
  "version": "1.0.3",
  "description": "Appengine client for Node.js",
  "repository": "googleapis/nodejs-appengine-admin",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google appengine",
    "appengine",
    "applications",
    "authorized certificates",
    "authorized domains",
    "domain mappings",
    "firewall",
    "instances",
    "services",
    "versions"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile-protos && npm run compile",
    "system-test": "c8 mocha build/system-test",
    "test": "c8 mocha build/test",
    "samples-test": "cd samples/ && npm link ../ && npm test",
    "prelint": "cd samples; npm link ../; npm i"
  },
  "dependencies": {
    "google-gax": "^2.17.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "@types/sinon": "^10.0.0",
    "c8": "^7.6.0",
    "gts": "^3.1.0",
    "jsdoc": "^3.6.6",
    "jsdoc-fresh": "^1.0.2",
    "jsdoc-region-tag": "^1.0.6",
    "linkinator": "^2.13.6",
    "mocha": "^8.3.1",
    "null-loader": "^4.0.1",
    "pack-n-play": "^1.0.0-2",
    "sinon": "^11.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": ">=v10.0.0"
  }
}
