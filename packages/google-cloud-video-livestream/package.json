{
  "name": "@google-cloud/livestream",
  "version": "0.1.0",
  "description": "livestream client for Node.js",
  "repository": "googleapis/nodejs-video-live-stream",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google livestream",
    "livestream",
    "livestream service"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prelint": "cd samples; npm link ../; npm install",
    "prepare": "npm run compile-protos && npm run compile",
    "system-test": "c8 mocha build/system-test",
    "test": "c8 mocha build/test",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../"
  },
  "dependencies": {
    "google-gax": "^2.29.4"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.44",
    "@types/sinon": "^10.0.0",
    "c8": "^7.7.2",
    "gts": "^3.1.0",
    "jsdoc": "^3.6.7",
    "jsdoc-fresh": "^1.0.2",
    "jsdoc-region-tag": "^1.0.6",
    "linkinator": "^3.0.3",
    "mocha": "^9.1.4",
    "null-loader": "^4.0.1",
    "pack-n-play": "^1.0.0-2",
    "sinon": "^10.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.7.0"
  },
  "engines": {
    "node": ">=v10.0.0"
  }
}
