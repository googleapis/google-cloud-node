{
  "name": "@google-cloud/kms",
  "description": "Google Cloud Key Management Service (KMS) API client for Node.js",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "googleapis/nodejs-kms",
  "main": "src/index.js",
  "files": [
    "protos",
    "src",
    "AUTHORS",
    "COPYING"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google kms",
    "kms",
    "Google Cloud Key Management Service (KMS) API"
  ],
  "scripts": {
    "test": "npm run cover",
    "generate-scaffolding": "repo-tools generate all",
    "generate-scaffolding-samples": "repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test-no-cover": "mocha test/ --timeout 600000",
    "lint": "eslint '**/*.js'",
    "docs": "jsdoc -c .jsdoc.js",
    "system-test": "mocha system-test/ smoke-test/ --timeout 600000",
    "fix": "eslint '**/*.js' --fix",
    "docs-test": "linkinator docs -r --skip www.googleapis.com",
    "predocs-test": "npm run docs"
  },
  "dependencies": {
    "google-gax": "^0.25.0",
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "codecov": "^3.0.4",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jsdoc-baseline": "git+https://github.com/hegemonic/jsdoc-baseline.git",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.0",
    "power-assert": "^1.6.0",
    "prettier": "^1.13.7",
    "linkinator": "^1.1.2"
  }
}
