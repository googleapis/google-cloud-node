{
  "name": "gapic-node-templating",
  "version": "1.0.0",
  "description": "Generates Node templates for gapic libraries",
  "bin": "./build/src/cli.js",
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "samples-test": "echo 'FIXME: no sample tests'",
    "system-test": "echo 'FIXME: no system tests'",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "test": "cross-env c8 mocha --exit build/test",
    "prelint": "npm install",
    "precompile": "gts clean"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.9",
    "@types/nunjucks": "^3.2.6",
    "@types/sinon": "^17.0.4",
    "@types/yargs": "^17.0.33",
    "c8": "^10.1.3",
    "cross-env": "^7.0.3",
    "gapic-tools": "^1.0.0",
    "gts": "^6.0.2",
    "mocha": "^11.1.0",
    "sinon": "^19.0.2",
    "snap-shot-it": "^7.9.10",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.15.2",
    "@octokit/rest": "^19.0.0",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.4",
    "yargs": "^17.7.2"
  }
}
