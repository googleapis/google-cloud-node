{
  "name": "gapic-node-templating",
  "version": "1.0.0",
  "description": "Generates Node templates for gapic libraries",
  "bin": "./build/src/cli.js",
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "test": "cross-env c8 mocha --exit build/test",
    "prelint": "npm install",
    "precompile": "gts clean"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "@octokit/rest": "^18.12.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/nunjucks": "^3.2.1",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.10",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.6",
    "sinon": "^14.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.1.0",
    "nunjucks": "^3.2.3",
    "octokit": "^1.7.2",
    "yargs": "^17.5.1"
  }
}
