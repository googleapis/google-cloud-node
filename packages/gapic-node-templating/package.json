    "@types/nunjucks": "^3.2.1",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.10",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "gapic-tools": "^0.2.0",
    "gts": "^5.0.0",
    "mocha": "^9.2.2",
    "sinon": "^15.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.0.0",
