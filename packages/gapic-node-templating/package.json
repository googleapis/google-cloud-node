{
  "name": "gapic-node-templating",
  "version": "1.0.0",
  "description": "Generates Node templates for gapic libraries",
  "bin": "./build/src/cli.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prelint": "cd samples; npm link ../; npm i",
    "prepare": "npm run compile-protos && npm run compile && minifyProtoJson",
    "samples-test": "npm run compile-test && cd samples/ && npm link ../ && npm i && npm test",
    "system-test": "npm run compile-test && c8 mocha build/system-test",
    "test": "c8 mocha build/test"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "@octokit/rest": "^19.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/nunjucks": "^3.2.1",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.10",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^9.2.2",
    "sinon": "^15.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.1.0",
    "@octokit/rest": "^19.0.0",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.3",
    "yargs": "^17.5.1"
  }
}