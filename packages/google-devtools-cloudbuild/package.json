{
  "repository": "googleapis/nodejs-cloudbuild",
  "name": "@google-cloud/cloudbuild",
  "version": "1.3.3",
  "author": "Google LLC",
  "description": "Cloud Build API client for Node.js",
  "main": "build/src/index.js",
  "browser": "src/browser.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google cloudbuild",
    "cloudbuild",
    "Cloud Build API"
  ],
  "devDependencies": {
    "@types/mocha": "^7.0.0",
    "c8": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gts": "^1.1.0",
    "jsdoc": "^3.6.2",
    "jsdoc-fresh": "^1.0.2",
    "jsdoc-region-tag": "^1.0.2",
    "linkinator": "^2.0.0",
    "mocha": "^7.0.0",
    "pack-n-play": "^1.0.0-2",
    "prettier": "^1.17.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "predocs": "npm run compile",
    "docs": "jsdoc -c .jsdoc.js",
    "lint": "gts check && eslint samples system-test",
    "clean": "gts clean",
    "fix": "gts fix && eslint samples system-test --fix",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "prepare": "npm run compile-protos && npm run compile",
    "pretest": "npm run compile",
    "test": "c8 mocha build/test",
    "system-test": "c8 mocha --timeout=15000 system-test/*.js",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "prelint": "cd samples; npm link ../; npm i"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "google-gax": "^1.7.5"
  }
}
