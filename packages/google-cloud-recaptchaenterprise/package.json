{
  "repository": "googleapis/nodejs-recaptcha-enterprise",
  "name": "@google-cloud/recaptcha-enterprise",
  "version": "1.1.1",
  "author": "Google LLC",
  "description": "reCAPTCHA Enterprise API client for Node.js",
  "main": "src/index.js",
  "files": [
    "protos",
    "src",
    "AUTHORS",
    "COPYING"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google recaptchaenterprise",
    "recaptchaenterprise",
    "reCAPTCHA Enterprise API"
  ],
  "dependencies": {
    "google-gax": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^9.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jsdoc": "^3.6.2",
    "jsdoc-baseline": "^0.1.0",
    "linkinator": "^1.4.2",
    "mocha": "^6.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "through2": "^2.0.3"
  },
  "scripts": {
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "docs": "jsdoc -c .jsdoc.js",
    "lint": "eslint '**/*.js'",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "system-test": "mocha system-test/*.js --timeout 600000",
    "test-no-cover": "mocha test/*.js",
    "test": "npm run cover",
    "fix": "eslint --fix '**/*.js'",
    "docs-test": "linkinator docs -r --skip 'www.npmjs.org/package/@google-cloud/recaptcha-enterprise|cloud.google.com/nodejs/docs/reference/recaptcha-enterprise/latest/|github.com/googleapis/nodejs-recaptcha-enterprise/tree/master/samples|https://github.com/googleapis/nodejs-recaptcha-enterprise/blob/master/samples/quickstart.js'",
    "predocs-test": "npm run docs"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.10.0"
  }
}
