{
  "repository": "googleapis/nodejs-containeranalysis",
  "name": "@google-cloud/containeranalysis",
  "version": "1.8.0",
  "author": "Google LLC",
  "description": "Container Analysis API client for Node.js",
  "main": "build/src/index.js",
  "files": [
    "build/protos",
    "build/src"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
