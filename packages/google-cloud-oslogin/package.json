{
  "name": "@google-cloud/os-login",
  "description": "Google Cloud OS Login API client for Node.js",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "author": "Google Inc",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "googleapis/nodejs-os-login",
  "main": "src/index.js",
  "files": [
    "protos",
    "src",
    "AUTHORS",
    "COPYING"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google os login",
    "os login",
    "Google Cloud OS Login API"
  ],
  "contributors": [
    "Alexander Fenster <fenster@google.com>",
    "Dave Gramlich <callmehiphop@gmail.com>",
    "Jason Dobry <jmdobry@users.noreply.github.com>",
    "Stephen <stephenplusplus@users.noreply.github.com>",
    "greenkeeper[bot] <greenkeeper[bot]@users.noreply.github.com>"
  ],
  "scripts": {
    "test": "npm run cover",
    "generate-scaffolding": "repo-tools generate all",
    "generate-scaffolding-samples": "repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test-no-cover": "mocha test/*.js",
    "lint": "eslint src/ samples/ system-test/ test/",
    "prettier": "prettier --write src/*.js src/*/*.js samples/*.js samples/*/*.js test/*.js test/*/*.js system-test/*.js system-test/*/*.js smoke-test/*.js",
    "docs": "jsdoc -c .jsdoc.js",
    "system-test": "mocha system-test/*.js smoke-test/*.js --timeout 600000"
  },
  "dependencies": {
    "google-gax": "^0.19.0",
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^2.2.4",
    "async": "^2.6.0",
    "codecov": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-prettier": "^2.4.0",
    "extend": "^3.0.1",
    "ink-docstrap": "^1.3.2",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.0",
    "nyc": "^13.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.9.2",
    "through2": "^2.0.3"
  }
}
