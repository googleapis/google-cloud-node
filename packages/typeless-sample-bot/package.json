{
  "name": "@google-cloud/typeless-sample-bot",
  "description": "Google Cloud GitHub bot that converts TypeScript snippets into JavaScript",
  "version": "1.3.1",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "googleapis/google-cloud-node",
  "bin": "./build/src/bin/typeless-sample-bot.js",
  "main": "./build/src/app.js",
  "type": "module",
  "files": [
    "build/src"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prelint": "cd samples; npm link ../; npm i",
    "prepare": "npm run compile-protos && npm run compile && minifyProtoJson",
    "samples-test": "npm run compile-test && cd samples/ && npm link ../ && npm i && npm test",
    "system-test": "npm run compile-test && c8 mocha build/system-test",
    "test": "c8 mocha build/test"
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.18.0",
    "@babel/traverse": "^7.18.10",
    "chalk": "^5.0.1",
    "debug": "^4.3.4",
    "recast": "^0.22.0",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/types": "^7.17.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__traverse": "^7.14.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.13",
    "@types/yargs": "^17.0.10",
    "c8": "^7.11.3",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "sinon": "^15.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "^4.6.3"
  }
}