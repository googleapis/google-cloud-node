    "proxyquire": "1.8.0",
    "sinon": "2.3.4"
  },
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "product": "resource",
    "samples": [
      {
        "id": "projects",
        "name": "Projects",
        "file": "projects.js",
        "docs_link": "https://cloud.google.com/resource-manager/docs",
        "usage": "node projects.js --help"
      }
    ]
