{
  "name": "@google-cloud/compute",
  "description": "Google Compute Engine Client Library for Node.js",
  "version": "0.10.0",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "googleapis/nodejs-compute",
  "main": "./src/index.js",
  "files": [
    "src",
    "AUTHORS",
    "CONTRIBUTORS",
    "LICENSE"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google compute engine",
    "compute engine"
  ],
  "scripts": {
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "docs": "jsdoc -c .jsdoc.js",
    "generate-scaffolding": "repo-tools generate all && repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "lint": "eslint '**/*.js'",
    "samples-test": "cd samples/ && npm link ../ && cd startup-script && npm link ../../ && cd ../ && npm test && cd ../",
    "system-test": "mocha system-test/*.js --timeout 600000",
    "test-no-cover": "mocha test/*.js",
    "test": "npm run cover",
    "fix": "eslint --fix '**/*.js'"
  },
  "dependencies": {
    "@google-cloud/common": "^0.28.0",
    "@google-cloud/paginator": "^0.1.1",
    "@google-cloud/projectify": "^0.3.0",
    "@google-cloud/promisify": "^0.3.1",
    "arrify": "^1.0.1",
    "async": "^2.6.1",
    "create-error-class": "^3.0.2",
    "extend": "^3.0.1",
    "gce-images": "^1.0.0",
    "is": "^3.2.1",
    "string-format-obj": "^1.1.1",
    "teeny-request": "^3.10.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "codecov": "^3.0.2",
    "concat-stream": "^1.6.2",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.0",
    "power-assert": "^1.5.0",
    "prettier": "^1.13.5",
    "propprop": "^0.3.1",
    "proxyquire": "^2.0.1",
    "uuid": "^3.2.1"
  }
}
