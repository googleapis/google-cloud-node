{
  "preset": "node-style-guide",
  "requireTrailingComma": null,
  "requireCapitalizedComments": null,
  "requireSpaceAfterKeywords": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "case",
    "return",
    "try",
    "typeof"
  ],
  "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
  "maximumLineLength": {
    "value": 80,
    "allowUrlComments": true
  },
  "excludeFiles": [
    "**/src/*/**/*",
    "**/system-test/*/**/*",
    "**/test/**/gapic-*.js",
    "**/test/*/**/*",
    "**/node_modules/",
    "**/.coverage/**/*.js",
  ]
}
